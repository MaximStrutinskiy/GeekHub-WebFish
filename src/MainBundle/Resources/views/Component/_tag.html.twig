<ul class="block-tags">
  {% if tags is not empty %}
    {% for tag in tags %}
      <li>
        <a href="{{ path('tag', {'id' : tag.id, 'name' : tag.name|replace ({' ': '_'})}) }}">
          {{ tag.name }};
        </a>
      </li>
    {% endfor %}
  {% else %}
    <p>No tags</p>
  {% endif %}
</ul>
