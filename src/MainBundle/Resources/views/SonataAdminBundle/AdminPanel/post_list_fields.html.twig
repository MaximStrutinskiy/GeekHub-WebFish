{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
  <div class="admin-panel-content-image">
    {% if object.postImg != null %}
      <img src="{% if object.postImg starts with 'http' %} {{ asset(object.postImg ) }} {% else %} {{ asset('post_images/' ~ object.postImg ) }} {% endif %}">
    {% endif %}
  </div>
{% endblock %}
